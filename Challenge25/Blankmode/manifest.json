{
  "manifest_version": 3,
  "name": "Blank Page Timer",
  "version": "1.1",
  "description": "Blanks out the webpage at custom intervals.",
  "permissions": [
    "activeTab",
    "scripting",
    "alarms",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon-16.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "blankmode.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}